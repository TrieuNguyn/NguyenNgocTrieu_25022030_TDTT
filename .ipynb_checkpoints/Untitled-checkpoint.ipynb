    "# W5A4\n",
    "from math import sqrt\n",
    "\n",
    "def isperfect(n):\n",
    "    s = 1\n",
    "    i = 2\n",
    "    while i*i <= n:\n",
    "        if n%i==0:\n",
    "            s += i\n",
    "        if n/i > i:\n",
    "            s += n/i\n",
    "        i += 1\n",
    "    return n==s\n",
    "\n",
    "def main():\n",
    "    n = int(input())\n",
    "    print(isperfect(n))\n",
    "\n",
    "main()"
   ]
  }
